{
  "name": "feather-agent",
  "version": "0.1.0",
  "description": "Tiny, fast, provider-agnostic LLM agent framework (orchestrator, fallback, race, rate limits, retries, streaming, cost, middleware).",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/src/bin/feather.js"
    }
  },
  "bin": {
    "feather": "dist/src/bin/feather.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest run",
    "test:ci": "npm ci && npm run test",
    "test:watch": "vitest",
    "examples:chat": "node dist/examples/chat.js",
    "examples:real-world": "node dist/examples/real-world-app.js",
    "examples:agent-chaining": "node dist/examples/agent-chaining.js",
    "lint": "tsc --noEmit",
    "prepack": "npm run build",
    "dashboard": "npm run build && node dist/src/dashboard/server.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "llm",
    "agent",
    "orchestrator",
    "openai",
    "anthropic",
    "chat",
    "stream",
    "rate-limit",
    "retry",
    "circuit-breaker",
    "fallback",
    "middleware",
    "cost-tracking"
  ],
  "author": "Open Source Community",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Geddydukes/feather-agent.git"
  },
  "bugs": {
    "url": "https://github.com/Geddydukes/feather-agent/issues"
  },
  "homepage": "https://github.com/Geddydukes/feather-agent#readme",
  "dependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.0.0"
  }
}
